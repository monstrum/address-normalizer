{
	"name" : "monstrum/address-normalizer",
	"repositories" : [],
	"require" : {
		"php" : "^7.0",
		"lukemadhanga/php-transliterator": "^0.1.1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "7.*",
		"pdepend/pdepend" : "2.5.*",
		"sebastian/phpcpd" : "4.*",
		"squizlabs/php_codesniffer": "3.*",
		"phploc/phploc" : "2.0.*",
		"phpmd/phpmd" : "2.6.*"
	},
	"suggest" : {
	},
	"autoload" : {
		"psr-4" : {
			"AddressNormalizer\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"AddressNormalizerTest\\" : "test/"
		}
	},
	"config" : {
		"secure-http" : false,
		"bin-dir" : "bin/",
		"discard-changes" : true
	}
}
